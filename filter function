           The filter() Array method creates a new array with elements that fall under a given criteria from an existing array.


           //filter function array of numbers
           
           let age = [18, 24, 36, 49, 10];
           
           let voters = age.filter(perfectVoters)
           
           function perfectVoters(agecheck){
               return agecheck >=15
           }
           
           console.log(voters);


           //filter function  array of objects
           
          let pokemon =[
           {name:"pikachu", power:"thunder"},
           {name:"charminar", power:"fire" },
           {name:"rhino", power:"thunder"}
           ];
           
           let findPower = pokemon.filter(find);
           
           function find(getPower){
               return getPower.power =="thunder";
           }
           
           console.log(findPower);
