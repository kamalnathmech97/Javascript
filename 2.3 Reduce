minimum number in array :

let arr=[-10, 2, 3, 20]

function reducerTest(accumulator, currentValue){
 if(accumulator < currentValue){
   currentValue = accumulator
 }
  return currentValue
}
let sum = arr.reduce(reducerTest, Number.MAX_VALUE)
console.log(sum)


let a = [5, 2, 8, 1, 3];
let reduceOperation = a.reduce(function(accumulator, currentValue) {
  if (currentValue < accumulator) {
    return currentValue;
  } else {
    return accumulator;
  }
});

console.log(reduceOperation); // Output: 1

